            "required": true,
            "type": "string",
            "default": "CPC",
            "format": "string"
          },
          {
            "name": "value",
            "in": "path",
            "description": "Caption parameter to retrieve from RMF III Report(if Available)",
            "required": true,
            "type": "string",
            "default": "CPCHCMSU",
            "format": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "JSON"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "deprecated": false
      }
    },
